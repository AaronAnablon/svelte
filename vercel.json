{
    "builds": [
      {
        "src": "src/routes/**/*.svelte",
        "use": "@sveltejs/kit",
        "config": {
          "extensions": [".svelte"],
          "distDir": "build"
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "/index.html" }
    ]
  }
  